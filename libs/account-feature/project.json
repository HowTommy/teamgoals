{
  "projectType": "library",
  "root": "libs/account-feature",
  "sourceRoot": "libs/account-feature/src",
  "prefix": "teamgoals",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/account-feature"],
      "options": {
        "jestConfig": "libs/account-feature/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/account-feature/src/**/*.ts", "libs/account-feature/src/**/*.html"]
      }
    }
  },
  "tags": ["scope:feature"]
}
